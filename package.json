{
  "name": "meshwork",
  "version": "0.0.26",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Multi-module package.json manager",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "meshwork": "./cli.js"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/meshwork"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/meshwork/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "util.chdir": "0.0.37",
    "util.merge-packages": "0.0.13",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/yargs": "^12.0.5",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "util.fixture": "0.0.65",
    "util.pkgwrap": "^0.0.94"
  },
  "pkgwrap": {
    "cleanup": [
      "index.js",
      "index.d.ts",
      "cli.js",
      "cli.d.ts"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "notify": false,
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/helpers"
    ],
    "verbose": false
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --jest"
  }
}
