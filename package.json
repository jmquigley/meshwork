{
  "name": "meshwork",
  "version": "0.0.23",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Multi-module package.json manager",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "meshwork": "./cli.js"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/meshwork"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/meshwork/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^4.0.2",
    "util.chdir": "0.0.33",
    "util.merge-packages": "0.0.10",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/fs-extra": "4.0.5",
    "@types/node": "^8.0.54",
    "@types/yargs": "^8.0.2",
    "ava": "^0.24.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "util.fixture": "0.0.61",
    "util.pkgwrap": "^0.0.88"
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run report",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
