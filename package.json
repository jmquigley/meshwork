{
  "name": "meshwork",
  "version": "0.0.25",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Multi-module package.json manager",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "meshwork": "./cli.js"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/meshwork"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/meshwork/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "util.chdir": "0.0.35",
    "util.merge-packages": "0.0.12",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.2",
    "@types/node": "^9.6.6",
    "@types/yargs": "^11.0.0",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "util.fixture": "0.0.63",
    "util.pkgwrap": "^0.0.92"
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run report",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
