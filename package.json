{
  "name": "meshwork",
  "version": "0.0.18",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Multi-module package.json manager",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/meshwork"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/meshwork/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "fs-extra": "^2.1.2",
    "util.chdir": "0.0.24",
    "util.merge-packages": "0.0.2",
    "util.pkgwrap": "^0.0.26",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "2.1.0",
    "@types/node": "^7.0.13",
    "@types/yargs": "^6.6.0",
    "ava": "^0.19.1",
    "coveralls": "^2.13.0",
    "nyc": "^10.2.0",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "util.fixture": "0.0.49"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "meshwork": "./cli.js"
  },
  "scripts": {
    "all": "npm install && npm run lint && npm run build && npm test && npm run report",
